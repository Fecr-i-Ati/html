						<div id="middle-wizard">
							<div class="step">
								<h2 class="section_title">Üretici Ürün Kayıt Formu</h2>
								<h3 class="main_question">Sağlık ürünü üreticisi ürün kayıt ekranı</h3>
								<div class="form-group add_top_30">
									<label for="urun_ismi">Ürün İsmi</label>
									<input type="text" name="urun_ismi" id="urun_ismi" class="form-control required">
								</div>
								<div class="form-group add_top_30">
									<label for="urun_durum">Ürün Durumu</label>
									<input type="text" name="urun_durum" id="urun_durum" class="form-control required">
								</div>
								<div class="form-group add_top_30">
									<label for="urun_durum">Ürün Onay Numarası</label>
									<input type="text" name="urun_durum" id="urun_durum" class="form-control required">
								</div>


							</div>
							<!-- /step-->

							<!-- /Start Branch ============================== -->






							<div class="submit step" id="end">
								<div class="summary">
									<div class="wrapper">
										<h3>Teşekkürler!</h3>
										<p>Ürününüz kaydedildi. </p>
									</div>
								</div>
							</div>
							<!-- /step last-->

						</div>
						<!-- /middle-wizard -->
						<div id="bottom-wizard">
							<button type="button" name="backward" class="backward">Önceki</button>
							<button type="button" name="forward" class="forward">Sonraki</button>
							<button type="submit" name="process" class="submit">Gönder</button>
						</div>
